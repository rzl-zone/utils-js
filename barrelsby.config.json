{
  "delete": true,
  "directory": "src",
  "exclude": [
    "deprecated$",
    "deprecated/",
    "deprecated\\.ts$",
    "deprecated",
    "private$",
    "private/",
    "private\\.ts$",
    "private",
    "_private$",
    "_private/",
    "_private\\.ts$",
    "_private",
    "browser$",
    "browser/",
    "browser\\.ts$",
    "browser",
    "/next",
    "/types/index\\.ts$",
    "types$",
    "types/",
    "types\\.ts$",
    "types"
  ],
  "extension": "ts",
  "location": "all",
  "prefix": "./",
  "single": false,
  "verbose": false
}
